{
    "name": "course-management",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:dev-static": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
        "start": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest  --runInBand",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage --detectOpenHandles",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "npm run build && npx typeorm -d dist/database/typeorm.migrations-data-source.js",
        "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/$npm_config_filename",
        "migration:create": "npm run build && npx typeorm migration:create src/database/migrations/$npm_config_filename",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:run:fake": "npm run typeorm -- migration:run --fake",
        "migration:revert": "npm run typeorm -- migration:revert"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.341.0",
        "@aws-sdk/s3-request-presigner": "^3.342.0",
        "@golevelup/ts-jest": "^0.3.7",
        "@nestjs/common": "^9.2.1",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.2.1",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.2.1",
        "@nestjs/sequelize": "^9.0.0",
        "@nestjs/swagger": "7.1.16",
        "@nestjs/throttler": "^4.0.0",
        "@nestjs/typeorm": "^9.0.1",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "module-alias": "^2.2.3",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.6.0",
        "typeorm": "^0.3.11"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.5",
        "@nestjs/schematics": "^9.0.3",
        "@nestjs/testing": "^9.2.1",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.14",
        "@types/jest": "29.2.4",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.11.12",
        "@types/sequelize": "^4.28.14",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "29.7.0",
        "jest-mock-extended": "3.0.5",
        "prettier": "^2.8.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "29.1.1",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s",
            "!<rootDir>/database/**/*.ts",
            "!<rootDir>/common/test/*.ts",
            "!<rootDir>/**/tests/*.ts",
            "!<rootDir>/config/**/*.ts",
            "!**/*.module.ts",
            "!<rootDir>/**/entities/**/*.ts"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^@app/(.*)$": "<rootDir>/../src/$1",
            "^@common/(.*)$": "<rootDir>/../src/common/$1"
        }
    },
    "_moduleAliases": {
        "^@app/(.*)$": "<rootDir>/../src/$1",
        "^@common/(.*)$": "<rootDir>/../src/common/$1"
    }
}
